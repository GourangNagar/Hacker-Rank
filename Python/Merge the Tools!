def merge_the_tools(string, k):
    n = int(len(string)/k)
    for i in range(n):
        sub = string[ i*k:(i+1)*k ]
        new=""
        for char in sub:
            if char not in new:
                new=new+char
        print(new)



if __name__ == '__main__':
    string, k = input(), int(input())
    merge_the_tools(string, k)
